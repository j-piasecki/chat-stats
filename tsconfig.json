{
  "compilerOptions": {
    "module": "ESNext",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "ES2017",
    "moduleResolution": "node",
    "types": ["jest", "node"],
    "esModuleInterop": true,
    "stripInternal": true
  }
}
